{
  "manifest_version": 3,
  "name": "Add Email Provider",
  "version": "1.0",
  "description": "Tired of typing @gmail.com? add this to your typed username in one click!",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "action": {
    "default_icon": "icon.png",
    "default_title": "Add Email Provider"
  },
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  }
}